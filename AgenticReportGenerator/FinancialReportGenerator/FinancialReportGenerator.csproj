<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="Azure.AI.OpenAI" Version="2.2.0-beta.4" />
	<PackageReference Include="Azure.AI.Projects" Version="1.0.0-beta.5" />
	<PackageReference Include="Azure.Identity" Version="1.14.0-beta.2" />
	<PackageReference Include="DotNetEnv" Version="3.1.1" />
	<PackageReference Include="Markdig" Version="0.40.0" />
	<PackageReference Include="Markdown2Pdf" Version="2.2.2" />
	<PackageReference Include="Microsoft.SemanticKernel" Version="1.42.0" />
	<PackageReference Include="Microsoft.SemanticKernel.Agents.AzureAI" Version="1.42.0-preview" />
	<PackageReference Include="Microsoft.SemanticKernel.Agents.Core" Version="1.42.0-preview" />
	<PackageReference Include="PuppeteerSharp" Version="20.1.3" />
 </ItemGroup>

 <ItemGroup>
	<None Update=".env">
		<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	</None>
 </ItemGroup>

</Project>
